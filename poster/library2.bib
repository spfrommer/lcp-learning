journal = {IEEE Transactions on Automatic Control},
keywords = {Dynamic running,Hybrid Zero Dynamics (HZD),Legged robots,Spring Loaded Inverted Pendulum (SLIP)},
number = {8},
pages = {1779--1793},
title = {{The spring loaded inverted pendulum as the hybrid zero dynamics of an asymmetric hopper}},
volume = {54},
year = {2009}
