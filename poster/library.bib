year = {2013}
}
@article{Todorov2014,
abstract = {We describe a full-featured simulation pipeline implemented in the MuJoCo physics engine. It includes multi-joint dynamics in generalized coordinates, holonomic constraints, dry joint friction, joint and tendon limits, frictionless and frictional contacts that can have sliding, torsional and rolling friction. The forward dynamics of a 27-dof humanoid with 10 contacts are evaluated in 0.1 msec. Since the simulation is stable at 10 msec timesteps, it can run 100 times faster than real-time on a single core of a desktop processor. Furthermore the entire simulation pipeline can be inverted analytically, an order-of-magnitude faster than the corresponding forward dynamics. We soften all constraints, in a way that avoids instabilities and unrealistic penetrations associated with earlier spring-damper methods and yet is sufficient to allow inversion. Constraints are imposed via impulses, using an extended version of the velocity-stepping approach. For holomonic constraints the extension involves a soft version of the Gauss principle. For all other constraints we extend our earlier work on complementarity-free contact dynamics - which were already known to be invertible via an iterative solver - and develop a new formulation allowing analytical inversion.},
author = {Todorov, Emanuel},
doi = {10.1109/ICRA.2014.6907751},
isbn = {9781479936847},
issn = {10504729},
journal = {Proceedings - IEEE International Conference on Robotics and Automation},
pages = {6054--6061},
title = {{Convex and analytically-invertible dynamics with contacts and constraints: Theory and implementation in MuJoCo}},
year = {2014}
}
@article{Todorov2010,
abstract = {Contact dynamics are commonly formulated as a linear complementarity problem. While this approach is superior to earlier spring-damper models, it can be inaccurate due to pyramid approximations to the friction cone, and inefficient due to lack of convexity coupled with a large number of auxiliary variables. Here we propose a new approach: implicit complementarity. Instead of treating contact velocities and forces as independent variables subject to explicit complementarity constraints, we express them as functions of a minimal set of unconstrained variables, and design these functions so that the complementarity constraints are automatically satisfied. We then solve the equations of motion via a non-smooth Gauss-Newton method augmented with an original linesearch procedure which exploits the problem structure. This enables us to represent the friction cone exactly and to reduce the number of unknowns by about a factor of 3. Numerical tests suggest that, in usage scenarios typical for robotics, the solver takes only about 5 iterations even without warm starts. More extensive tests and side-by-side comparisons remain to be done, but nevertheless the potential of the new approach is clear.},
author = {Todorov, Emanuel},
doi = {10.1109/ROBOT.2010.5509739},
isbn = {9781424450381},
issn = {10504729},
journal = {Proceedings - IEEE International Conference on Robotics and Automation},
number = {5},
pages = {2322--2329},
title = {{Implicit nonlinear complementarity: A new approach to contact dynamics}},
year = {2010}
}
@article{Jia1999,
abstract = {In the absence of vision, grasping an object often relies on tactile$\backslash$nfeedback from the fingertips. As the finger pushes the object, the$\backslash$nfingertip can feel the contact point move. If the object is known in$\backslash$nadvance, from this motion the finger may infer the location of the$\backslash$ncontact point on the object, and thereby the object pose. This paper$\backslash$nprimarily investigates the problem of determining the pose (orientation$\backslash$nand position) and motion (velocity and angular velocity) of a planar$\backslash$nobject with known geometry from such contact motion generated by$\backslash$npushing.$\backslash$nA dynamic analysis of pushing yields a nonlinear system that relates$\backslash$nthrough contact the object pose and motion to the finger motion. The$\backslash$ncontact motion on the fingertip, thus encodes certain information about$\backslash$nthe object pose. Nonlinear observability theory is employed to show that$\backslash$nsuch information is sufficient for the finger to ``observe{\{}''{\}} not only$\backslash$nthe pose, but also the motion of the object. Therefore, a sensing$\backslash$nstrategy can be realized as an ``observer{\{}''{\}} of the nonlinear dynamical$\backslash$nsystem. Two observers are subsequently introduced. The first observer;$\backslash$nbased on the work of Gauthier, Hammouri, and Othman (1992), has its$\backslash$n``gain{\{}''{\}} determined by the solution of a Lyapunov-like equation; it$\backslash$ncan be activated at any time instant during a push. The second observes$\backslash$nbased on Newton's method, solves for the initial (motionless) object$\backslash$npose from three intermediate contact points during a push.$\backslash$nUnder the Coulomb-friction model, the paper deals with support friction$\backslash$nin the plane and/or contact friction between the finger and the object.$\backslash$nExtensive simulations have been done to demonstrate the feasibility of$\backslash$nthe two observers. Preliminary experiments (with an Adept robot) have$\backslash$nalso been conducted A contact sensor has been implemented using strain$\backslash$ngauges.},
author = {Jia, Y B and Erdmann, M},
doi = {10.1177/027836499901800504},
issn = {0278-3649},
journal = {International Journal of Robotics Research},
number = {5},
pages = {466--490},
title = {{Pose and motion from contact}},
volume = {18},
year = {1999}
}
