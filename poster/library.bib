journal = {The International Journal of Robotics Research},
keywords = {balance,humanoid robots,m ulti-contact motions,optimization,taylor series approximation,time-interval discretization},
number = {9-10},
pages = {1104--1119},
title = {{Generation of whole-body optimal dynamic multi-contact motions}},
url = {http://ijr.sagepub.com/cgi/doi/10.1177/0278364913478990},
volume = {32},
year = {2013}
}
