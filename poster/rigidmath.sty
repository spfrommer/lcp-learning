\newcommand{\LCPVariables}[0]{\vect{z}}
\newcommand{\LCP}[2]{\mathrm{LCP}(#1, #2)}
\newcommand{\LCPSolutionSet}[2]{\mathrm{SOL}(#1, #2)}
\newcommand{\LCPSet}[2]{\LCPSol{#1}{#2}}
\newcommand{\Complementary}[2]{\ZeroVector \leq #1 \perp #2 \geq \ZeroVector}



